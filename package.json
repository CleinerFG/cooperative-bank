{
  "name": "cooperative-bank",
  "version": "1.0.0",
  "scripts": {
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "install:databases": "cd databases && npm install",
    "install": "npm run install:client && npm run install:server && npm run install:databases",
    "dev:client": "npm --prefix client run dev",
    "dev:server": "npm --prefix server run dev",
    "start:client": "npm --prefix client run build",
    "start:server": "npm --prefix server run start",
    "set:databases": "npm --prefix server run set:databases"
  },
  "keywords": [
    "Bank",
    "Finance",
    "Transactions",
    "Loans",
    "Payments",
    "Investments",
    "HTML",
    "CSS",
    "JS",
    "Webpack",
    "Babel",
    "Express",
    "API",
    "Rest",
    "SPA",
    "MySQL",
    "SQL"
  ],
  "author": "Cleiner Furlani",
  "license": "ISC",
  "description": "A simple banking simulation app for managing transactions, loans, and payments between people"
}
